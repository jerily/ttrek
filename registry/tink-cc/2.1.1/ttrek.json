{
	"name": "tink-cc",
	"version": "2.1.1",
	"scripts": {
	},
	"dependencies":	{
		"openssl": "3.2.1",
		"abseil-cpp": "20230802.1",
		"protobuf":	"21.9"
	},
	"devDependencies": {
	},
	"lifecycle": {
		"install": [
			{ "cmd": "curl", "url": "https://github.com/tink-crypto/tink-cc/archive/refs/tags/v2.1.1.tar.gz" },
			{ "cmd": "unpack" },
			{ "cmd": "cd" },
			{ "cmd": "patch", "p_num": "1", "filename": "tink-cc-2.1.1.diff" },
			{
				"cmd": "cmake_config",
				"options": [
					{ "name": "TINK_BUILD_SHARED_LIB", "value": "ON" },
					{ "name": "TINK_USE_INSTALLED_ABSEIL", "value": "ON" },
					{ "name": "TINK_USE_SYSTEM_OPENSSL", "value": "ON" },
					{ "name": "TINK_USE_INSTALLED_PROTOBUF", "value": "ON" },
					{ "name": "TINK_USE_INSTALLED_RAPIDJSON", "value": "OFF" },
					{ "name": "TINK_BUILD_TESTS", "value": "OFF" },
					{ "name": "CMAKE_SKIP_RPATH", "value": "ON" },
					{ "name": "CMAKE_BUILD_TYPE", "value": "Release" }
				]
			},
			{ "cmd": "make_install" }
		]
	}
}